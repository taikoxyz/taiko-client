  "version": "0.2.0",
  "configurations": [
    {
      "name": "driver",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/main.go",
      "args": [
        "driver",
        "--verbosity=4",
        "--l1.ws=ws://localhost:18546",
        "--l2.ws=ws://localhost:28546",
        "--taikoL1=0x0DCd1Bf9A1b36cE34237eEaFef220932846BCD82",
        "--taikoL2=0x1000777700000000000000000000000000000001",
        "--jwtSecret=${workspaceFolder}/integration_test/nodes/jwt.hex",
        "--l2.auth=http://localhost:28551"
      ]
    },
    {
      "name": "proposer",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/main.go",
      "args": [
        "proposer",
        "--verbosity=4",
        "--l1.ws=ws://localhost:18546",
        "--l2.http=http://localhost:28545",
        "--taikoL1=0x0DCd1Bf9A1b36cE34237eEaFef220932846BCD82",
        "--taikoL2=0x1000777700000000000000000000000000000001",
        "--blockProposalFee=1000",
